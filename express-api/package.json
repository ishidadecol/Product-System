{
  "name": "express-api",
  "version": "1.0.0",
  "description": "Express api for a small product system ",
  "homepage": "https://github.com/ishidadecol/Product-System#readme",
  "bugs": {
    "url": "https://github.com/ishidadecol/Product-System/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ishidadecol/Product-System.git"
  },
  "license": "ISC",
  "author": "Daniel Ishida",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/index.js",
    "migration:generate": "npx sequelize-cli migration:generate",
    "migrate": "npx env-cmd -f ../.env sequelize-cli db:migrate --config ./config/config.cjs",
    "migrate:rollback": "env-cmd npx sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "env-cmd": "^10.1.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "yup": "^1.6.1"
  }
}
